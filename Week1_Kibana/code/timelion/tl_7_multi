# readable code
.es(
  index=shopping, 
  timefield=주문시간, 
  metric=sum:상품가격, 
  q=고객성별:남성
), 
.es(
  index=shopping, 
  timefield=주문시간, 
  metric=sum:상품가격, 
  q=고객셩별:남성, 
  offset=-1M
)

